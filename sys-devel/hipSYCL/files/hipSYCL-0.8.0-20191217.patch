+class spin_lock {
+public:
+  void lock() {
+    while (_lock.test_and_set(std::memory_order_acquire));
+  }
+  void unlock() {
+    _lock.clear(std::memory_order_release);
+  }
+private:
+  std::atomic_flag _lock = ATOMIC_FLAG_INIT;
+};
+
+class spin_lock_guard {
+public:
+  spin_lock_guard(spin_lock& lock) : _lock(lock) {
+    _lock.lock();
+  }
+  ~spin_lock_guard() {
+    _lock.unlock();
+  }
+private:
